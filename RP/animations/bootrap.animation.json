{
	"format_version": "1.8.0",
	"animations": {
		"animation.bootrap.look_at_target": {
			"loop": true,
			"bones": {
				"body": {
					"rotation": ["(query.target_x_rotation - this) / 4", "(query.target_y_rotation - this) / 2", 0]
				}
			}
		},
		"animation.bootrap.idle": {
			"loop": true,
			"animation_length": 1,
			"bones": {
				"root": {
					"position": [0, "math.sin(query.life_time * 180.0) + 1", "math.cos(query.life_time * 360.0) / -4"]
				},
				"top_cover": {
					"rotation": {
						"0.0": {
							"post": [0, 0, 0],
							"lerp_mode": "catmullrom"
						},
						"0.875": {
							"post": [-45, 0, 0],
							"lerp_mode": "catmullrom"
						},
						"1.0": [0, 0, 0]
					}
				},
				"bottom_cover": {
					"rotation": {
						"0.0": {
							"post": [0, 0, 0],
							"lerp_mode": "catmullrom"
						},
						"0.875": {
							"post": [45, 0, 0],
							"lerp_mode": "catmullrom"
						},
						"1.0": [0, 0, 0]
					}
				}
			}
		},
		"animation.bootrap.move": {
			"loop": true,
			"animation_length": 0.5,
			"anim_time_update": "query.modified_distance_moved / 20",
			"bones": {
				"root": {
					"position": [0, "math.sin(query.life_time * 360.0) * 2 + 1", "math.cos(query.life_time * 720.0) / -2"]
				},
				"top_cover": {
					"rotation": {
						"0.0": {
							"post": [0, 0, 0],
							"lerp_mode": "catmullrom"
						},
						"0.375": {
							"post": [-45, 0, 0],
							"lerp_mode": "catmullrom"
						},
						"0.5": [0, 0, 0]
					}
				},
				"bottom_cover": {
					"rotation": {
						"0.0": {
							"post": [0, 0, 0],
							"lerp_mode": "catmullrom"
						},
						"0.375": {
							"post": [45, 0, 0],
							"lerp_mode": "catmullrom"
						},
						"0.5": [0, 0, 0]
					}
				}
			}
		}
	}
}